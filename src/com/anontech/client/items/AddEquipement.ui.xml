<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<fieldset>
		<legend>Add Equipement</legend>
			<g:Grid width="428px" height="294px">
				<g:row>
					<g:customCell>
						<g:Label text="Description"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Make"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Model"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Category"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox width="172px"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Note"/>
					</g:customCell>
					<g:customCell>
						<g:TextArea width="256px" height="109px"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Button>Save</g:Button>
					</g:customCell>
				</g:row>
			</g:Grid>
		</fieldset>
	</g:HTMLPanel>
</ui:UiBinder> 